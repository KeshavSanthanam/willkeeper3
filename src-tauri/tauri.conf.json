{
  "productName": "Willkeeper",
  "version": "0.1.0",
  "identifier": "com.yourorg.willkeeper",

  "build": {
    "beforeDevCommand": "npm run dev:ui",
    "beforeBuildCommand": "npm run build:ui",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../out"
  },

  "app": {
    "withGlobalTauri": true,
    "security": { "csp": null, "capabilities": [] },
    "windows": [
      {
        "title": "Willkeeper",
        "width": 1200,
        "height": 800,
        "resizable": true
      }
    ]
  },

  "bundle": {},
  "plugins": {}
}
